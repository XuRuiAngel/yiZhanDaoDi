<!--pages/my/my.wxml-->
<view class="view_contain">
  <!-- 第一部分 -->
  <view class="view_1">
    <view class="view_image_text">
      <view class="userinfo-avatar">
        <open-data  type="userAvatarUrl"></open-data>
      </view>
      <open-data type="userNickName"></open-data>
    </view>
  </view>
  <!-- 第二部分 -->

  <!-- 第三部分 -->
  <view class="view_3">
    <view class="list-item">
      <image class="item-image" src="../../image/xingbie.jpg"></image>
      <text class="item-text" >性别:</text>  
      <text class="item-text" >{{user.sex}}</text>
      <image class="image-jiantou" src="../../image/jiantou.jpg"></image>
    </view>
    <view class="line"></view>
    <view class="list-item">
      <image class="item-image" src="../../image/touxian.jpg"></image>
      <text class="item-text">头衔:</text>
      <text class="item-text" >{{user.title}}</text>
      <image class="image-jiantou" src="../../image/jiantou.jpg"></image>
    </view>
    <view class="line"></view>
    <view class="list-item">
      <image class="item-image" src="../../image/dianhua.jpg"></image>
      <text class="item-text">电话:</text>
      <text class="item-text" >{{user.tel}}</text>
      <image class="image-jiantou" src="../../image/jiantou.jpg"></image>
    </view>
    <view class="line"></view>
    <view class="list-item">
      <image class="item-image" src="../../image/jinbi.jpg"></image>
      <text class="item-text">得分:</text>
      <text class="item-text" >{{user.score}}</text>
      <image class="image-jiantou" src="../../image/jiantou.jpg"></image>
    </view>
    <view class="line"></view>

    <view class="line"></view>
  </view>
</view>
